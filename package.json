  "bugs": {
    "url": "https://github.com/aborazmeh/turkish-wordlist/issues"
  },
  "homepage": "https://github.com/aborazmeh/turkish-wordlist#readme",
  "devDependencies": {
    "prettier": "^3.3.2",
    "axios": "^1.7.2"
  }
}
